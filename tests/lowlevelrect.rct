package sys;

external malloc(size int) pointer[byte];
external free(mem pointer[byte]);

while (true) {
   var mem <- make pointer[byte] array(0);

   from (i <- 0) to 10000 {
      sys::Print("mwhahahahahaHAHAHAHAHAHAHA");
      mem->Push(malloc(1000));
      sys::Sleep(1);
   }

   from (i <- 0) to 10000 {
      free(mem[i]);
   }
}

sys::Input();
