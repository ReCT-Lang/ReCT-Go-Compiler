// much class
class SomeClass {

	set int i;

	function coolFunc(suffix string) {
		var someMessage <- "very cool class function inside a class such wow";
		Print(someMessage);
		Print(suffix);
	}

	function anotherFunction() {
		coolFunc("much wow");
		i++;
	}

	function aThirdFunction() {
		anotherFunction();
		coolFunc("yes");

		Print(string(i));
	}
}


